    "format": "prettier --write src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "seed": "tsx scripts/seed.ts",
    "migrate": "tsx scripts/migrate.ts",
    "generate-keys": "tsx generate-keys.ts"
