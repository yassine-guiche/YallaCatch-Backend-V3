{
  "name": "yallacatch-backend-complete",
  "version": "1.0.0",
  "description": "YallaCatch! Complete Backend API - Production Ready",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "rimraf dist && tsc",
    "start": "node dist/server.js",
    "start:prod": "pm2 start ecosystem.config.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --config jest.e2e.config.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "docs": "typedoc src --out docs/api",
    "seed": "tsx scripts/seed.ts",
    "migrate": "tsx scripts/migrate.ts",
    "docker:build": "docker build -t yallacatch-backend .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "k8s:deploy": "kubectl apply -f deployment/kubernetes/",
    "monitoring:setup": "tsx scripts/setup-monitoring.ts",
    "backup": "tsx scripts/backup.ts",
    "restore": "tsx scripts/restore.ts"
  },
  "dependencies": {
    "fastify": "^4.24.3",
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.0.1",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^2.0.0",
    "@fastify/multipart": "^8.0.0",
    "@fastify/static": "^6.12.0",
    "@fastify/websocket": "^8.3.1",
    "mongoose": "^8.0.3",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "argon2": "^0.31.2",
    "zod": "^3.22.4",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "dotenv": "^16.3.1",
    "node-cron": "^3.0.3",
    "bull": "^4.12.0",
    "nodemailer": "^6.9.7",
    "twilio": "^4.19.3",
    "firebase-admin": "^11.11.1",
    "@aws-sdk/client-s3": "^3.454.0",
    "sharp": "^0.32.6",
    "uuid": "^9.0.1",
    "nanoid": "^5.0.4",
    "dayjs": "^1.11.10",
    "lodash": "^4.17.21",
    "@turf/turf": "^6.5.0",
    "rate-limiter-flexible": "^3.0.8",
    "speakeasy": "^2.0.0",
    "qrcode": "^1.5.3",
    "node-fetch": "^3.3.2",
    "axios": "^1.6.2",
    "node-geocoder": "^4.2.0",
    "i18next": "^23.7.6",
    "i18next-fs-backend": "^2.3.1",
    "accept-language-parser": "^1.5.0"
  },
  "devDependencies": {
    "typescript": "^5.3.2",
    "@types/node": "^20.9.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/uuid": "^9.0.7",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "@types/qrcode": "^1.5.5",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.16",
    "mongodb-memory-server": "^9.1.1",
    "redis-memory-server": "^0.7.0",
    "tsx": "^4.6.0",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.5",
    "eslint": "^8.54.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "prettier": "^3.1.0",
    "typedoc": "^0.25.4",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "semantic-release": "^22.0.8",
    "pm2": "^5.3.0",
    "cross-env": "^7.0.3",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "keywords": [
    "yallacatch",
    "ar",
    "geolocation",
    "gaming",
    "tunisia",
    "backend",
    "api",
    "nodejs",
    "typescript",
    "mongodb",
    "redis",
    "fastify"
  ],
  "author": "Manus AI",
